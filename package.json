{
  "name": "leajs-server",
  "description": "extendable, configuration based http server",
  "version": "0.1.0",
  "homepage": "https://github.com/lea-js/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lea-js/leajs-server"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/lea.js",
  "bin": {
    "leajs": "cli.js"
  },
  "files": [
    "lib/*.js",
    "cli.js"
  ],
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "script-runner": "^0.1.7",
    "snapy": "^0.1.3"
  },
  "keywords": [],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "coffee --bare --no-header --compile --output lib/ src/*.coffee",
    "dev": "npm run build && ./cli.js",
    "doc": "toDoc",
    "test": "snapy",
    "preversion": "npm run test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^6.0.1",
    "ip": "^1.1.5",
    "yaku": "^0.18.6",
    "hook-up": "^0.2.2",
    "read-conf": "^0.4.3"
  }
}